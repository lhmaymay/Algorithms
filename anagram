# example: check if a given string is an anagram of another given string.
# SEA, DATABASE TRUE
# BAT, DATABASE TRUE
# DAB, DATABASE FALSE

def is_anagram(str1, str2):
    list_str1 = list(str1)
    list_str1.sort()
    list_str2 = list(str2)
    list_str2.sort()

    return (list_str1 == list_str2)

print(is_anagram('anagram','nagaram'))
# True
print(is_anagram('cat','rat'))
# False
