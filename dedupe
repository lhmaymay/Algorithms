def deduplicate(l):
    count = {}
    (read,write) = (0,0)
    while read < len(l):
        if l[read] in count:
            read += 1
            continue
        count[l[read]] = True
        l[write] = l[read]
        read += 1
        write += 1
    return l[0:write]
deduplicate([1,1,2,3,1,4,1])


seen = set()
result = []
foo="aabaccda"
for c in foo:
    if c not in seen:
        result.append(c)
        seen.add(c)
result = ''.join(result)
